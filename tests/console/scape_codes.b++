// importa la libreria std
import "stdlib.b++";

// este programa requiere Colors se encuentra en stdlib.b++
require Colors.new;

// crear los colores ASCII
Colors.new(ASCII);

out Mensaje = ASCII.green;
out Mensaje $= "Hello ";
out Mensaje $= ASCII.cyan;
out Mensaje $= "World";

Sys.Out Mensaje
